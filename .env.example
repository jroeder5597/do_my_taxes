# Tax Document Processor - Environment Variables
# Copy this file to .env and fill in your values

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=gpt-oss:20b

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333

# OCR Service Configuration (containerized Tesseract)
# Set to use containerized OCR service instead of local Tesseract
# OCR_SERVICE_URL=http://localhost:5000

# Database
DATABASE_PATH=db/taxes.db

# Logging
LOG_LEVEL=INFO

# Optional: API Keys (if using cloud services in future)
# OPENAI_API_KEY=sk-xxx
# ANTHROPIC_API_KEY=sk-xxx